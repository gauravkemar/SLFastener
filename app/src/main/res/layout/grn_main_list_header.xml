<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp">

    <TextView
        android:id="@+id/tvSrNo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Sr No"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvKGRNNO"
        app:layout_constraintHorizontal_weight="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tvKGRNNO"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="KGRN NO"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvKGRNDate"
        app:layout_constraintHorizontal_weight="0.11"
        app:layout_constraintStart_toEndOf="@+id/tvSrNo"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tvKGRNDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="KGRN Date"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvInvoiceNo"
        app:layout_constraintHorizontal_weight="0.07"
        app:layout_constraintStart_toEndOf="@+id/tvKGRNNO"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tvInvoiceNo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Invoice No"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvInvoiceDate"
        app:layout_constraintHorizontal_weight="0.10"
        app:layout_constraintStart_toEndOf="@+id/tvKGRNDate"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tvInvoiceDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Inv. Date"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvBPCode"
        app:layout_constraintHorizontal_weight="0.07"
        app:layout_constraintStart_toEndOf="@+id/tvInvoiceNo"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tvBPCode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="BP Code"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvBPCodeName"
        app:layout_constraintHorizontal_weight="0.10"
        app:layout_constraintStart_toEndOf="@+id/tvInvoiceDate"
        app:layout_constraintTop_toTopOf="parent">


    </TextView>

    <TextView
        android:id="@+id/tvBPCodeName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Supplier"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvType"
        app:layout_constraintHorizontal_weight="0.20"
        app:layout_constraintStart_toEndOf="@+id/tvBPCode"
        app:layout_constraintTop_toTopOf="parent">


    </TextView>

    <TextView
        android:id="@+id/tvType"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Txn Type"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvCurrency"
        app:layout_constraintHorizontal_weight="0.06"
        app:layout_constraintStart_toEndOf="@+id/tvBPCodeName"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tvCurrency"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Currency"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvStatus"
        app:layout_constraintHorizontal_weight="0.05"
        app:layout_constraintStart_toEndOf="@+id/tvType"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tvStatus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Status"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvAction"
        app:layout_constraintHorizontal_weight="0.07"
        app:layout_constraintStart_toEndOf="@+id/tvCurrency"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tvAction"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Actions"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_text"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="0.05"
        app:layout_constraintStart_toEndOf="@+id/tvStatus"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

</androidx.constraintlayout.widget.ConstraintLayout>



